{
  "$schema": "https://deno.land/x/denon@2.4.7/schema.json",
  "scripts": {
    "start": {
      "cmd": "deno run api.ts",
      "desc": "run my api.ts file",
      "allow": {
        "net": true,
        "env": true,
        "write": true,
        "read": true
      },
      "unstable": false
    }
  },
  "logger": {
    "fullscreen": false,
    "quiet": false,
    "debug": true
  }
}
